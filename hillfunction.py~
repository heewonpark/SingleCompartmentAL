#! /usr/bin/python

import matplotlib.pyplot as plt
import numpy as np
import math
def Res(c):
    K = 6000
    Rmax = 170
    n = 1.5
    if(c != 0):
        r = Rmax/(1+math.pow(K/c , n))
    else:
        r = 0
    return r

size = 90000
R = [Res(i) for i in np.arange(0,size,0.2)]
x = np.arange(0,size,0.2)

#print R
fig = plt.figure()
plt.plot(x,R)
#plt.xscale('log')
plt.savefig("hill.png")
plt.show()
